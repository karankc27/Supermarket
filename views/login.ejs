<%-include("header")%>
<!--banner-->
<div class="banner-top">
 <div class="container">
   <h3 >Login</h3>
   <% if(errorMessage) { %>
   		<div class="alert alert-success" role="alert"><B><%=errorMessage%></B></div>
   		<% } %>


   <h4><a href="/">Home</a><label>/</label>Login</h4>

   <div class="clearfix"> </div>
 </div>
</div>
<!--login-->

 <div class="login">

   <div class="main-agileits">
       <div class="form-w3agile">
         <h3>Login</h3>
         <form action="/login" method="post">
           			<input type="hidden" name="_csrf" value="<%= csrfToken%>">
           <div class="key">
             <i class="fa fa-envelope" aria-hidden="true"></i>
             <input  type="text"  name="email" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Email';}" required="">
             <div class="clearfix"></div>
           </div>
           <div class="key">
             <i class="fa fa-lock" aria-hidden="true"></i>
             <input  type="password"  name="password" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Password';}" required="">
             <div class="clearfix"></div>
           </div>
           <input type="submit" value="Login">
         </form>
       </div>
       <div class="forg">
         <a href="/register" class="forg-right">Register</a>
       <div class="clearfix"></div>
       </div>
     </div>
   </div>
<!--footer-->
<%-include("footer")%>
